{
  "manifest_version": 2,

  "name": "Titleer (2.2 Iron)",
  "description": "Titleer changes tab title to a random number",
  "version": "2.2",
  "icons": { "128": "128icon.png" },
  "browser_action": {
   "default_icon": "128icon.png",
   "default_popup": "popup.html"
  },
    "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
   "tabs", "activeTab", "<all_urls>"
   ],
  "commands": {
    "title": {
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "mac": "Command+Shift+K"
      },
      "description": "Change title!"
    }
  }
}